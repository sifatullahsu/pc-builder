{
  "name": "pc-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx .",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "lint-prettier": "yarn lint:check && yarn prettier:check",
    "husky:setup": "yarn husky install && yarn husky add .husky/pre-commit \"yarn lint-staged\""
  },
  "dependencies": {
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "next": "13.5.2",
    "next-auth": "^4.23.1",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "daisyui": "^3.7.7",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3"
  }
}
